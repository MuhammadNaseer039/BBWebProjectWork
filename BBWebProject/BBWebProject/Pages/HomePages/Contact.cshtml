    <link href="~/css/style.css" rel="stylesheet">
    <style>
        .color {
            padding: 10px; /* Adjust padding as needed */
        }

            .color img {
                height: 300px;
                width: 100px;
            }

        * {
            user-select: none;
        }
    </style>
